{
  "tests": [
    {
      "name": "Register returns error for duplicate email",
      "method": "POST",
      "path": "/",
      "body": {
        "action": "register",
        "email": "admin@example.com",
        "password": "password123",
        "name": "Test User"
      },
      "expectedStatus": 400,
      "expectedBody": {
        "error": "string"
      },
      "bodyMatcher": "partial"
    },
    {
      "name": "Login with existing user",
      "method": "POST",
      "path": "/",
      "body": {
        "action": "login",
        "email": "admin@example.com",
        "password": "admin123"
      },
      "expectedStatus": 200,
      "expectedBody": {
        "success": true,
        "session_token": "string"
      },
      "bodyMatcher": "partial"
    }
  ]
}